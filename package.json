{
  "name": "forcedotcom-builder",
  "main": "./lib/build",
  "version": "0.5.5",
  "private": true,
  "description": "Work on your Force.com projects, straight from Atom",
  "activationCommands": {
    "atom-workspace": [
      "force.com:generate-project",
      "force.com:go-to-wiki",
      "force.com:deploy-current-file",
      "force.com:deploy-file-treeview",
      "force.com:deploy-project",
      "force.com:deploy-static-res",
      "force.com:deploy-apex",
      "force.com:deploy-visualforce",
      "force.com:deploy-selected-files",
      "force.com:abort",
      "force.com:retrieve-project",
      "force.com:retrieve-current-file",
      "force.com:retrieve-file-treeview",
      "force.com:retrieve-selected-files",
      "force.com:new-apex-class",
      "force.com:new-apex-trigger",
      "force.com:new-vf-page",
      "force.com:new-vf-component",
      "force.com:create-custom-label-editor",
      "force.com:create-custom-label-project"
    ]
  },
  "repository": "https://github.com/jonathanrico/forcedotcom-builder",
  "license": "MIT",
  "engines": {
    "atom": ">=0.50.0"
  },
  "dependencies": {
    "atom-message-panel": ">=1.0.0",
    "atom-space-pen-views": "~2.2.0",
    "space-pen": "*"
  }
}
